{
    "name": "ecotone/laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": ["ddd","cqrs","messaging","eip", "distributed architecture","ecotone", "ddd and cqrs on top of eip"],
    "description": "Laravel integration for Ecotone",
    "autoload": {
        "psr-4": {
            "Ecotone\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "ecotone/ecotone": "^9.0",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0",
        "behat/behat": "^3.3.1",
        "phan/phan": "^1.2.0",
        "guzzlehttp/psr7": "^1.4",
        "php-coveralls/php-coveralls": "^2.1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ecotone\\Laravel\\EcotoneProvider"
            ],
            "aliases": {
            }
        }
    }
}
